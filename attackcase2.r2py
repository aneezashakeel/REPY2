# Attack: Test space restriction - SILENT VERSION

for fn in ("sp1.txt", "sp1.txt.a", "sp1.txt.b"):
    if fn in listfiles():
        removefile(fn)

myfile = ABopenfile("sp1.txt", True)
myfile.writeat(" starts", 0)
myfile.close()

for fn in ("sp2.txt", "sp2.txt.a", "sp2.txt.b"):
